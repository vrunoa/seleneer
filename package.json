{
  "name": "seleneer",
  "version": "0.0.1",
  "private": true,
  "description": "Experiments combining Selenium & Puppeter",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./tests",
    "start-selenium": "java -jar ./bin/selenium-server-standalone-3.11.0.jar &",
    "test-animations-enabled": "PLAYBACK_RATE=2 node_modules/.bin/mocha -b -t 0 tests/test_animations.js",
    "test-animations-disabled": "PLAYBACK_RATE=0 node_modules/.bin/mocha -b -t 0 tests/test_animations.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vrunoa/seleneer.git"
  },
  "keywords": [
    "selenium",
    "puppetter",
    "test",
    "automation"
  ],
  "author": " <vruno.alassia@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vrunoa/seleneer/issues"
  },
  "homepage": "https://github.com/vrunoa/seleneer#readme",
  "devDependencies": {
    "asyncbox": "^2.5.3",
    "babel-eslint": "^10.0.2",
    "chai": "^4.2.0",
    "eslint": "^6.1.0",
    "husky": "^3.0.2",
    "mocha": "^6.2.0",
    "puppeteer-core": "^1.19.0",
    "wd": "^1.11.3"
  }
}
